{
  "name": "unit-viewer-react",
  "version": "0.5.4",
  "description": "A react component to display junit xml test file",
  "main": "./dist/index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "test": "echo \"Error: no test specified\" && exit 0",
    "transpile": "babel src -d dist --copy-files",
    "prepublishOnly": "npm run transpile",
    "build": "webpack --mode production",
    "deploy": "gh-pages -d examples/dist",
    "publish-demo": "npm run build && npm run deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paultrh/unit-viewer-react.git"
  },
  "keywords": [
    "react",
    "unit",
    "junit",
    "xml",
    "tests"
  ],
  "author": "paultrh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paultrh/unit-viewer-react/issues"
  },
  "homepage": "https://github.com/paultrh/unit-viewer-react#readme",
  "dependencies": {
    "@material-ui/core": "^4.0.2",
    "@material-ui/icons": "^4.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "titlecase": "^1.1.3",
    "uuid": "^3.3.2",
    "webpack": "^4.33.0",
    "xml2js-parser": "^1.1.1"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^3.0.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "style-loader": "^0.20.3",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.1.3"
  }
}
